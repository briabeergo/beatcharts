<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AvatarView">
        <attr name="avatarBackgroundColor" format="color" />
    </declare-styleable>
    <declare-styleable name="TextRunningView">
        <attr name="text" format="string" />
        <attr name="speed" format="integer" />
        <attr name="textSize" format="dimension" />
        <attr name="direction" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="MovingSquaresView">
        <attr name="sqrSpeed" format="float" />
        <attr name="frequency" format="integer" />
        <attr name="squareSize" format="dimension" />
        <attr name="moveRight" format="boolean" />
        <attr name="squareColor" format="color" />
    </declare-styleable>
</resources>